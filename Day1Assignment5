
public class BankAccount {

	
	public static void main(String[] args) {
	BankName bn=new BankName(1234,50000,"priya2000");
	bn.displayAccount();
	}

}
class BankName
{
	private int acno;
	private int acbalance;
	private String acpassword;
	BankName(int acno,int acbalance,String acpassword)
	{
		this.acno=acno;
		this.acbalance=acbalance;
		this.acpassword=acpassword;
	}
	void displayAccount()
	{
		System.out.println("Account number is "+acno);
		System.out.println("Balance amount is the account is "+acbalance);
		System.out.println("Account password is "+acpassword);
	}
}
